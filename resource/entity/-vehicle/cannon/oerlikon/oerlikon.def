{game_entity
	(include "/properties/cannon.ext")
	{props "vehicle" "cannon" "cannon_small" "flak" "barrel1" "round_sandbag2" "vision_cannon_minimum"}
	{collider "cannon_small"}
        {targetSelector "cannon_shell_flak"}
		
	("armor_light")		
	
	{placer	{place "gunner" {UseVision 0}}}
	
	("wheel" args "wheelright")
	("wheel" args "wheelleft")

	("crew_wheeled_gun")
	{boarder
		{anm "gunner"
			{forward	{end "pose_flak38-1_gunlayer" 10}}
		}
		{anm "commander"
			{forward	{end "mgun_stan_charger2" 10}}
		}
		{anm "driver1"
			{forward {end "cannon_pak40_pusher_left" 10}}
		}
		{anm "driver2"
			{forward {end "cannon_pak40_pusher_right" 10}}
		}
		{anm "seat1"
			{forward {end "pose_flak38-1_gunlayer" 10}}
		}
		{anm "seat2"
			{forward {end "pose_flak38-1_charger" 10}}
		}
	}
	
	{mass 300}
	{Extension "oerlikon.mdl"}
	(include "/properties/selection/vehicle.inc"  scale(0.7))
	
	{extender "cannon"
		{animation
			{switch "close"}
			{move "cannon_pak40_pusher_left" "cannon_pak40_pusher_right"}
			{reload "Cannon_oerlikon_charger"}
		}
		{move {obstacle "close"}}
		{attack {obstacle "open"}}
	}
	{extender "inventory"
		{box
			{size 8 6}
			{item "bullet20_oerlikon zenite" 100}
			{item "bullet20_oerlikon ap" 100}
			{item "bullet20_oerlikon fg" 100}
		    {item "sandbag_kit2" 25}
;           {item "plant" 5}
		}
	}
	{Weaponry
		{place "gun"
			{weapon "20mm_oerlikon" filling "bullet20_oerlikon zenite" 60}
			("abm_zenite")
		}
	}

	{Chassis "track"
		("car_trace" fx("tracewheel01a") step(0.08) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("loco_small")
	}
	
	{bone "turret"
		{speed2 35}	
	}		
	{bone "gun_rot"
		{speed2 20}
	}	
	
}