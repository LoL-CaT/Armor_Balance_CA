{game_entity
	(include "/properties/cannon.ext")
	{props "vehicle" "cannon" "cannon_small" "wheeled" "round_sandbag5" "vision_cannon_medium" "muzzle_maximum"}
	{PatherID "cannon.large"}
	("armor_heavy")
	{collider "cannon_large"}

	("wheel" args "wheelright")
	("wheel" args "wheelleft")

	("crew_wheeled_gun")
	{boarder
		{anm "gunner"
			{forward	{end "pose_pak40_gunlayer" 10}}
		}
		{anm "commander"
			{forward	{end "pose_pak40_charger" 10}}
		}
		{anm "driver1"
			{forward {end "cannon_pak40_pusher_left" 10}}
		}
		{anm "driver2"
			{forward {end "cannon_pak40_pusher_right" 10}}
		}
		{anm "seat1"
			{forward {end "pose_seat_cannon1_left" 10}}
		}
		{anm "seat2"
			{forward {end "pose_seat_cannon1_right" 10}}
		}
	}

	{extender "inventory"
		{box
            {item "ammo bullet150_sig33 fg" 4}
;            {item "ammo bullet150_sig33 heat" 2}
		    {item "sandbag_kit2" 25}
;           {item "plant" 5}
		}
	}
	{Weaponry
		{place "gun"
			{weapon "150mm_sig33a" filling "bullet150_sig33 fg" 1}  
			{Rotate} 
			("abm_howitzer")
		}
	}

	{mass 1750}
	{Extension "sig33.mdl"}
	(include "/properties/selection/cannon.inc"  scale(0.7))

	{bone "turret"
		{limits -6 6}
		{speed2 5}
	}
	{bone "gun_rot"
		{limits -4 73}
		{speed2 5}
	}

		{extender "cannon"
			{animation
				{switch "close"}
				{move "cannon_pak40_pusher_left" "cannon_pak40_pusher_right"}
				{aim "cannon_pak40_gunlayer" ""}
				{fire  "Cannon_gunlayer_fire-alert" "cannon_zis3_charger_fire"}
				{reload  "" "cannon_pak40_charger and cannon_pak40_charger"}
			}
			{move {obstacle "close"}}
			{attack {obstacle "open"}}
		}

	{Chassis "track"
		("car_trace" fx("tracewheel05") step(0.07) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("loco_small")
	}
}
