		{door "emit1"}
		{link "emit0" "driver" {anm "driver"} {forward putoff} {reverse puton}}
	}
	("crew_human" place(commander)	door1(emit1))
	("crew_human" place(gunner)	    door1(emit1))
	("crew_human" place(charger)	door1(emit1))

